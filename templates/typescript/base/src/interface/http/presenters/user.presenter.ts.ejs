<% if (features.includes('auth')) { %>
import { User } from '../../../domain/entities/user';

export const userPresenter = {
  toHttp(user: User) {
    return {
      id: user.id,
      name: user.name,
      email: user.email,
      role: user.role,
      createdAt: user.createdAt.toISOString(),
      updatedAt: user.updatedAt.toISOString(),
    };
  },
};
<% } %>
